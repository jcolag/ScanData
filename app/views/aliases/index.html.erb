<p id="notice"><%= notice %></p>

<h1>Listing Aliases</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Document ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @aliases.each do |al| %>
      <tr>
        <td><%= al.name %></td>
        <td><%= al.document_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
